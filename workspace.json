{
  "version": 2,
  "projects": {
    "devkit-extra": {
      "root": "packages/devkit-extra",
      "sourceRoot": "packages/devkit-extra/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/devkit-extra/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/devkit-extra"
          ],
          "options": {
            "jestConfig": "packages/devkit-extra/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/devkit-extra",
            "tsConfig": "packages/devkit-extra/tsconfig.lib.json",
            "packageJson": "packages/devkit-extra/package.json",
            "main": "packages/devkit-extra/src/index.ts",
            "assets": [
              "packages/devkit-extra/*.md"
            ]
          }
        }
      }
    },
    "firebase-react": {
      "root": "packages/firebase-react",
      "sourceRoot": "packages/firebase-react/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/firebase-react",
            "tsConfig": "packages/firebase-react/tsconfig.lib.json",
            "project": "packages/firebase-react/package.json",
            "entryFile": "packages/firebase-react/src/index.ts",
            "external": [
              "react/jsx-runtime"
            ],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "packages/firebase-react/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/firebase-react/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/firebase-react"
          ],
          "options": {
            "jestConfig": "packages/firebase-react/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-hasura": {
      "root": "packages/nx-hasura",
      "sourceRoot": "packages/nx-hasura/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "packages/nx-hasura/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/nx-hasura"
          ],
          "options": {
            "jestConfig": "packages/nx-hasura/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/nx-hasura",
            "tsConfig": "packages/nx-hasura/tsconfig.lib.json",
            "packageJson": "packages/nx-hasura/package.json",
            "main": "packages/nx-hasura/src/index.ts",
            "assets": [
              "packages/nx-hasura/*.md",
              {
                "input": "./packages/nx-hasura/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-hasura/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./packages/nx-hasura",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/nx-hasura",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "nx-hasura-e2e": {
      "root": "e2e/nx-hasura-e2e",
      "projectType": "application",
      "sourceRoot": "e2e/nx-hasura-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "nx-hasura:build",
            "jestConfig": "e2e/nx-hasura-e2e/jest.config.js"
          }
        }
      }
    },
    "docs": {
      "projectType": "application",
      "root": "e2e/docs",
      "sourceRoot": "e2e/docs/src",
      "targets": {
        "build": {
          "executor": "@nx-plus/docusaurus:browser",
          "options": {
            "outputPath": "dist/e2e/docs"
          }
        },
        "serve": {
          "executor": "@nx-plus/docusaurus:dev-server",
          "options": {
            "port": 3000
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    }
  }
}
