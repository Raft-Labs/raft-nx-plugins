{
  "npmScope": "raftlabs",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "devkit-extra": {
      "tags": []
    },
    "docs": {
      "tags": []
    },
    "firebase-react": {
      "tags": []
    },
    "hbp-react": {
      "tags": []
    },
    "hbp-sdk": {
      "tags": []
    },
    "nx-hasura": {
      "tags": []
    },
    "nx-hasura-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-hasura"]
    },
    "react-hook-form-antd": {
      "tags": []
    }
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "packages"
  }
}
